{
  "name": "@semajsx/core",
  "version": "0.1.0",
  "description": "Core runtime for SemaJSX - VNode system and rendering primitives",
  "keywords": [
    "jsx",
    "rendering",
    "runtime",
    "vnode"
  ],
  "license": "MIT",
  "author": "lidessen",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@semajsx/signal": "workspace:*",
    "@semajsx/utils": "workspace:*"
  },
  "devDependencies": {
    "@semajsx/configs": "workspace:*",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
