{
  "name": "@semajsx/server",
  "version": "0.1.0",
  "description": "Server-side rendering and Island architecture for SemaJSX",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@semajsx/core": "workspace:*",
    "@semajsx/dom": "workspace:*",
    "@semajsx/logger": "workspace:*",
    "@semajsx/signal": "workspace:*"
  },
  "devDependencies": {
    "@semajsx/configs": "workspace:*",
    "tsdown": "^0.16.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
