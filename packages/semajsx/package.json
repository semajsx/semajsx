{
  "name": "semajsx",
  "version": "0.1.0",
  "description": "A signal-based reactive JSX runtime for building modern web applications",
  "keywords": [
    "framework",
    "jsx",
    "reactive",
    "signal",
    "typescript",
    "ui"
  ],
  "license": "MIT",
  "author": "lidessen",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./src/index.ts",
    "./dom": "./src/dom/index.ts",
    "./dom/jsx-dev-runtime": "./src/dom/jsx-dev-runtime.ts",
    "./dom/jsx-runtime": "./src/dom/jsx-runtime.ts",
    "./signal": "./src/signal/index.ts",
    "./ssg": "./src/ssg/index.ts",
    "./ssr": "./src/ssr/index.ts",
    "./ssr/client": "./src/ssr/client.ts",
    "./style": "./src/style/index.ts",
    "./style/react": "./src/style/react.ts",
    "./style/vue": "./src/style/vue.ts",
    "./tailwind": "./src/tailwind/index.ts",
    "./terminal": "./src/terminal/index.ts",
    "./terminal/jsx-dev-runtime": "./src/terminal/jsx-dev-runtime.ts",
    "./terminal/jsx-runtime": "./src/terminal/jsx-runtime.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./dom": "./dist/dom/index.mjs",
      "./dom/jsx-dev-runtime": "./dist/dom/jsx-dev-runtime.mjs",
      "./dom/jsx-runtime": "./dist/dom/jsx-runtime.mjs",
      "./signal": "./dist/signal/index.mjs",
      "./ssg": "./dist/ssg/index.mjs",
      "./ssr": "./dist/ssr/index.mjs",
      "./ssr/client": "./dist/ssr/client.mjs",
      "./style": "./dist/style/index.mjs",
      "./style/react": "./dist/style/react.mjs",
      "./style/vue": "./dist/style/vue.mjs",
      "./tailwind": "./dist/tailwind/index.mjs",
      "./terminal": "./dist/terminal/index.mjs",
      "./terminal/jsx-dev-runtime": "./dist/terminal/jsx-dev-runtime.mjs",
      "./terminal/jsx-runtime": "./dist/terminal/jsx-runtime.mjs",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:unit:run": "vitest run --config vitest.unit.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
    "test:terminal": "vitest run tests/terminal --config vitest.unit.config.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "ansi-escapes": "^7.2.0",
    "chalk": "^5.6.2",
    "cli-boxes": "^4.0.1",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "lightningcss": "^1.31.1",
    "minimatch": "^10.1.1",
    "slice-ansi": "^7.1.2",
    "string-width": "^8.1.0",
    "wrap-ansi": "^9.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@semajsx/configs": "workspace:*",
    "@semajsx/core": "workspace:*",
    "@semajsx/dom": "workspace:*",
    "@semajsx/logger": "workspace:*",
    "@semajsx/signal": "workspace:*",
    "@semajsx/ssg": "workspace:*",
    "@semajsx/ssr": "workspace:*",
    "@semajsx/style": "workspace:*",
    "@semajsx/tailwind": "workspace:*",
    "@semajsx/terminal": "workspace:*",
    "@semajsx/utils": "workspace:*",
    "@types/bun": "latest",
    "@types/node": "^25.0.10",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "playwright": "^1.58.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "react": ">=18.0.0",
    "vite": "^7.0.0",
    "vue": ">=3.0.0",
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "peerDependenciesMeta": {
    "@mdx-js/mdx": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "yoga-layout-prebuilt": {
      "optional": true
    }
  }
}
