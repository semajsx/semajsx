{
  "name": "semajsx",
  "version": "0.1.0",
  "description": "A signal-based reactive JSX runtime for building modern web applications",
  "keywords": [
    "framework",
    "jsx",
    "reactive",
    "signal",
    "typescript",
    "ui"
  ],
  "license": "MIT",
  "author": "lidessen",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./src/index.ts",
    "./dom": "./src/dom/index.ts",
    "./dom/jsx-dev-runtime": "./src/dom/jsx-dev-runtime.ts",
    "./dom/jsx-runtime": "./src/dom/jsx-runtime.ts",
    "./signal": "./src/signal/index.ts",
    "./terminal": "./src/terminal/index.ts",
    "./terminal/jsx-dev-runtime": "./src/terminal/jsx-dev-runtime.ts",
    "./terminal/jsx-runtime": "./src/terminal/jsx-runtime.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./dom": "./dist/dom/index.mjs",
      "./dom/jsx-dev-runtime": "./dist/dom/jsx-dev-runtime.mjs",
      "./dom/jsx-runtime": "./dist/dom/jsx-runtime.mjs",
      "./signal": "./dist/signal/index.mjs",
      "./terminal": "./dist/terminal/index.mjs",
      "./terminal/jsx-dev-runtime": "./dist/terminal/jsx-dev-runtime.mjs",
      "./terminal/jsx-runtime": "./dist/terminal/jsx-runtime.mjs",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:unit:run": "vitest run --config vitest.unit.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
    "test:terminal": "vitest run tests/terminal --config vitest.unit.config.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build",
    "example:dev": "cd examples/vite-counter && vite",
    "example:build": "cd examples/vite-counter && bun run build",
    "example:bun": "bun --conditions=development --port=0 examples/bun-server/server.tsx",
    "example:perf": "bun --conditions=development --port=0 examples/performance-test/server.tsx",
    "example:ssr": "bun --conditions=development examples/ssr-islands/server.tsx",
    "example:terminal": "bun --conditions=development examples/terminal-counter/simple.tsx",
    "example:terminal-print": "bun --conditions=development examples/terminal-print/simple-print.tsx",
    "example:logger": "bun --conditions=development examples/logger/basic.tsx",
    "example:logger:showcase": "bun --conditions=development examples/logger/showcase.tsx"
  },
  "dependencies": {
    "ansi-escapes": "^7.2.0",
    "chalk": "^5.6.2",
    "cli-boxes": "^4.0.1",
    "slice-ansi": "^7.1.2",
    "string-width": "^8.1.0",
    "wrap-ansi": "^9.0.2"
  },
  "devDependencies": {
    "@semajsx/configs": "workspace:*",
    "@semajsx/core": "workspace:*",
    "@semajsx/dom": "workspace:*",
    "@semajsx/signal": "workspace:*",
    "@semajsx/style": "workspace:*",
    "@semajsx/terminal": "workspace:*",
    "@semajsx/utils": "workspace:*",
    "@types/bun": "latest",
    "@types/node": "^25.0.10",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "playwright": "^1.58.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "yoga-layout-prebuilt": "^1.10.0"
  },
  "peerDependenciesMeta": {
    "yoga-layout-prebuilt": {
      "optional": true
    }
  }
}
