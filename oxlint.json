{
  "jsPlugins": ["./.oxlint/plugins/oxlint-plugin-english.js"],
  "rules": {
    "no-unused-vars": "warn",
    "no-debugger": "warn",
    "no-console": "warn",
    "no-warning-comments": "warn",
    "english-only/no-non-english-comments": "error"
  },
  "ignore": ["dist", "node_modules", "coverage", "*.tsbuildinfo", "playwright-report", "test-results"]
}
