{
  "name": "semajsx",
  "version": "0.1.0",
  "description": "A signal-based reactive JSX runtime for building modern web applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "types": "./dist/jsx-dev-runtime.d.ts"
    },
    "./signal": {
      "import": "./dist/signal/index.js",
      "types": "./dist/signal/index.d.ts"
    },
    "./dom": {
      "import": "./dist/dom/index.js",
      "types": "./dist/dom/index.d.ts"
    },
    "./dom/jsx-runtime": {
      "import": "./dist/dom/jsx-runtime.js",
      "types": "./dist/dom/jsx-runtime.d.ts"
    },
    "./dom/jsx-dev-runtime": {
      "import": "./dist/dom/jsx-dev-runtime.js",
      "types": "./dist/dom/jsx-dev-runtime.d.ts"
    },
    "./terminal": {
      "import": "./dist/terminal/index.js",
      "types": "./dist/terminal/index.d.ts"
    },
    "./terminal/jsx-runtime": {
      "import": "./dist/terminal/jsx-runtime.js",
      "types": "./dist/terminal/jsx-runtime.d.ts"
    },
    "./terminal/jsx-dev-runtime": {
      "import": "./dist/terminal/jsx-dev-runtime.js",
      "types": "./dist/terminal/jsx-dev-runtime.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json && tsc --noEmit -p tsconfig.dom.json && tsc --noEmit -p tsconfig.cli.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint src tests",
    "lint:fix": "oxlint --fix src tests",
    "test": "vitest",
    "test:unit": "vitest --config vitest.unit.config.ts",
    "test:unit:run": "vitest run --config vitest.unit.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --config vitest.unit.config.ts",
    "test:terminal": "vitest run tests/terminal --config vitest.unit.config.ts",
    "example:dev": "cd examples/vite-counter && vite",
    "example:build": "cd examples/vite-counter && npm run build",
    "example:bun": "bun --port=0 examples/bun-server/server.tsx",
    "example:perf": "bun --port=0 examples/performance-test/server.tsx",
    "example:terminal": "bun examples/terminal-counter/simple.tsx",
    "prepare": "husky"
  },
  "keywords": [
    "jsx",
    "signal",
    "reactive",
    "ui",
    "framework",
    "typescript"
  ],
  "author": "lidessen",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "@vitest/browser": "^4.0.8",
    "@vitest/browser-playwright": "^4.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "oxlint": "^1.28.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "packageManager": "bun@1.2",
  "private": true,
  "dependencies": {
    "ansi-escapes": "^7.2.0",
    "chalk": "^5.6.2",
    "cli-boxes": "^4.0.1",
    "slice-ansi": "^7.1.2",
    "string-width": "^8.1.0",
    "wrap-ansi": "^9.0.2",
    "yoga-layout-prebuilt": "^1.10.0"
  }
}
